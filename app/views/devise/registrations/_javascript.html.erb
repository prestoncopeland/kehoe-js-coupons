<script type="text/javascript" src="https://js.stripe.com/v2/"></script>
<script type="text/javascript">
Stripe.setPublishableKey("<%= Rails.application.secrets.stripe_publishable_key or fail 'No Stripe publishable key found.' %>")
</script>
<script type="text/javascript">
  console.log("JavaScript on the registrations#new page.");
  if (typeof Stripe != 'undefined') {
    console.log('Stripe file loaded.');
  }
  else
  {
    console.log('Problem. Stripe file not loaded.');
  }
  $(document).ready(function () {
    console.log('jQuery ready.');
  });
  console.log("Stripe key: <%= Rails.application.secrets.stripe_publishable_key %>");
</script>